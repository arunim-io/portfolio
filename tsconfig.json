{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error",
    "jsx": "react-jsx",
    "outDir": "./dist",
    "types": ["astro/client", "vite/client", "react", "react-dom"],
    "baseUrl": "src"
  }
}

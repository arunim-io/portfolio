{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error",
    "outDir": "./dist",
    "types": ["astro/client", "@astrojs/image/client"],
    "baseUrl": "src",
    "paths": {
      "~/*": ["./*"]
    }
  }
}

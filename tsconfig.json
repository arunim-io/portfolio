{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "strict": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "error",
    "sourceMap": true,
    "esModuleInterop": true,
    "lib": ["ESNext"],
    "outDir": "./dist",
    "types": ["astro/client", "@astrojs/image/client", "unplugin-icons/types/svelte"],
    "baseUrl": "src",
    "paths": {
      "~/*": ["./*"]
    }
  }
}

{
  "extends":["../.eslintrc","plugin:react/recommended", "plugin:react/jsx-runtime"],
  "parserOptions": { "ecmaFeatures": { "jsx": true } },
  "plugins": ["react"],
  "rules": {
    "react/jsx-sort-props": [
      "warn",
      {
        "callbacksLast": true,
        "shorthandFirst": true,
        "noSortAlphabetically": false,
        "reservedFirst": true
      }
    ]
  },
  "settings": { "react": { "version": "detect" } }

}
